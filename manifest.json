{
   "background": {
      "persistent": true,
      "scripts": [ "js/jquery-3.2.1.min.js", "js/background.js" ]
   },
   "browser_action": {
      "default_icon": "img/icon.png",
      "default_popup": "popup.html",
      "default_title": "SHOWROOM 人間をやめるぜ！！"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "js/jquery-3.2.1.min.js", "js/jquery-ui.min.js", "js/datepicker-ja.js", "js/common.js", "js/dom.js", "js/live_room.js", "js/live_open.js", "js/live_owner.js", "js/follow.js", "js/fanroom.js", "js/live_booking.js" ],
      "matches": [ "https://www.showroom-live.com/*" ],
      "run_at": "document_end"
   } ],
   "description": "目指せ、オワタ丸さん。",
   "icons": {
      "128": "img/icon.png",
      "16": "img/icon.png",
      "48": "img/icon.png"
   },
   "manifest_version": 2,
   "name": "SHOWROOM 人間をやめるぜ！！",
   "permissions": [ "storage", "notifications", "alarms", "tabs", "background" ],
   "short_name": "すこぷら",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.0.0",
   "web_accessible_resources": [ "*" ]
}
